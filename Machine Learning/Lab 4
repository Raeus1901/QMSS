        "model_1.add(Dense(units=3, activation='softmax'))  # Output layer with 3 outputs for classification\n",
        "\n",
        "# Compile the model\n",
        "model_1.compile(loss='categorical_crossentropy', optimizer='adam', metrics=['accuracy'])\n",
